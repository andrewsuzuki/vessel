{
    "name": "Vessel",
    "description": "Vessel: simple content management for Laravel 4.",
    "authors": [
        {
            "name": "Andrew Suzuki",
            "homepage": "http://andrewsuzuki.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "twig/twig": "1.*",
        "cartalyst/sentry": "2.1.*"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/controllers",
            "src/models",
            "src/migrations"
        ],
        "psr-0": {
            "Hokeo\\Vessel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}