{
    "name": "Vessel",
    "description": "Vessel: simple content management for Laravel 4.",
    "authors": [
        {
            "name": "Andrew Suzuki",
            "homepage": "http://andrewsuzuki.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/controllers",
            "src/models",
            "src/library",
            "src/migrations",
            "src/seeds",
            "src/formatters"
        ],
        "psr-0": {
            "Hokeo\\Vessel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}